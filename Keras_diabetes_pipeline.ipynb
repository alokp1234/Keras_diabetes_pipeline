    "    #     mlflow.pyfunc.log_model(model, \"model_logged\", keras_model_kwargs={\"save_format\": \"h5\"})\n",
    "        \n",
    "    import h5py\n",
    "    model.save(f'keras/model.h5')\n",
    "    \n",
    "    print(\"\\n Keras Sequential is trained on Pima-diabetes data and saved to PV location /keras/model.pkl ----\")"
